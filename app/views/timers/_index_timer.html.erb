<div class="card">
<p id="notice"><%= notice %></p>

<!-- <p>Timers</p> -->

<table class="card-content">
  <thead class=" ">
    <tr>
      <th>Time</th>
      <th>Description</th>
      <th>Job</th>
      <!-- <th colspan="3"></th> -->
    </tr>
  </thead>

  <tbody>
    <%  @timers.each do |timer| %>
      <tr>
        <td><%= timer.time_entry %></td>
        <td><%= timer.description %></td>
        <td><%= timer.job %></td>
        <td><%#= link_to 'Show', timer %></td>
        <td><%#= link_to 'Edit', edit_timer_path(timer) %></td>
        <td><%#= link_to 'Destroy', timer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>

